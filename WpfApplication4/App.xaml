<Application x:Class="WpfApplication4.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Image x:Key="Play" Source="Images/play.jpg" Height="40" Width="50" />
        <Image x:Key="Stop" Source="Images/pause.jpg" Height="40" Width="50"/>
        <Image x:Key="no_repeat" Source="Images/no_repeat.jpg" Height="40" Width="50" />
        <Image x:Key="repeat_all" Source="Images/repeat_all.jpg" Height="40" Width="50"/>
        <Image x:Key="repeat_song" Source="Images/repeat_song.png" Height="40" Width="50"/>
    </Application.Resources>
</Application>
